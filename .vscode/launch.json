	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "godot",
			"request": "launch",
			"project": "${workspaceFolder}",
			"address": "127.0.0.1",
			"port": 6007,
			"scene": "main",
			"editor_path": "D:/software/godot/Godot_v4.4.1-stable_win64.exe/Godot_v4.4.1-stable_win64.exe",
